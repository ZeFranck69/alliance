<div class='header-content main-wrapper'>
    <div class='header--menu-container left'>
        <nav class='nav--left__links'>
            {% include 'partial/menu.twig' with {'items': menu.header.left.get_items } %}
        </nav>
    </div>
    <div class='header--menu-container right'>
       
        <nav class='nav--right__links'>
            {% include 'partial/menu.twig' with {'items': menu.header.right.get_items} %}
            {% include 'partial/socials.twig' %}
        </nav>
        
        
    </div>
</div>
<div class="header--mobile">
